{
  "jmad-model-definition": {
    "@name": "SPS", 
    "svn-revision": "$Revision$", 
    "default-optic": {
      "@ref-name": "default optics"
    }, 
    "init-files": {
      "call-file": [
        {
          "@path": "init-options.madx", 
          "@location": "RESOURCE"
        }, 
        {
          "@path": "beams/LHC_beam_injection.beamx"
        }, 
        {
          "@path": "elements/sps2009.ele"
        }, 
        {
          "@path": "aperture/aperturedb_1_2009.dbx"
        }, 
        {
          "@path": "aperture/aperturedb_2_2009.dbx"
        }, 
        {
          "@path": "sequence/sps2009.seq"
        }, 
        {
          "@path": "aperture/aperturedb_3_2009.dbx"
        }, 
        {
          "@path": "strength/lhc_newwp_2009.str"
        }, 
        {
          "@path": "strength/elements.str"
        }
      ]
    }, 
    "path-offsets": {
      "repository-offset": {
        "@value": "sps/optics2009/SPS/SPSRing/2009"
      }, 
      "resource-offset": {
        "@value": "sps"
      }
    }, 
    "sequences": {
      "sequence": {
        "ranges": {
          "range": {
            "madx-range": {
              "@first": "#s", 
              "@last": "#e"
            }, 
            "twiss-initial-conditions": {
              "chrom": {
                "@value": "true"
              }, 
              "centre": {
                "@value": "false"
              }, 
              "@name": "default-twiss", 
              "closed-orbit": {
                "@value": "false"
              }
            }, 
            "@name": "ALL"
          }
        }, 
        "default-range": {
          "@ref-name": "ALL"
        }, 
        "@name": "sps"
      }
    }, 
    "optics": {
      "optic": {
        "@overlay": "false", 
        "init-files": {
          "call-file": {
            "@path": "strength/lhc_newwp_2009.str", 
            "@parse": "STRENGTHS"
          }
        }, 
        "@name": "default optics"
      }
    }, 
    "default-sequence": {
      "@ref-name": "sps"
    }
  }
}